{
  "name": "bear-md-export",
  "version": "1.0.0",
  "description": "The plugin helps you to export your notes from Bear to Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "bear",
    "export",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "fast-glob": "^3.2.12"
  }
}
